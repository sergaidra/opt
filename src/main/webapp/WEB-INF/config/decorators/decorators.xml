<?xml version="1.0" encoding="UTF-8"?>
<decorators defaultdir="/WEB-INF/jsp/">

	<decorator name="layout" page="layout.jsp">
		<pattern>/*</pattern>
	</decorator>

	<excludes>
		<pattern>/index*</pattern>
		
		<!-- 관리자 -->
		<pattern>/mngr/*</pattern>

		<!-- 관리자 -->
		<pattern>/admin/*</pattern>

		<!-- 항공API정보 -->
		<pattern>/flight/*</pattern>
		
		<!-- gmap -->
		<pattern>/gmap/*</pattern>

		<!-- iframe -->
		<pattern>*Iframe/*</pattern>

		<!-- popup -->
		<pattern>*Popup/*</pattern>

		<!-- action -->
		<pattern>*Action/*</pattern>

		<!-- json -->
		<pattern>*Json/*</pattern>

		<pattern>/html/*</pattern>


	</excludes>
</decorators>
